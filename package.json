    "es6",
    "helpers",
    "last",
    "order",
    "orderBy",
    "promise",
    "promisify",
    "promisifyAll",
    "sort",
    "sortBy",
    "toEnum",
    "utility"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^4.0.2",
    "codacy-coverage": "^2.0.2",
    "copy-webpack-plugin": "^4.0.1",
