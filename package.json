    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "webpack": "3.5.5"
  },
